{
  "name": "@poche/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for Poche applications",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "require": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./util": {
      "types": "./src/util.ts",
      "import": "./src/util.ts",
      "require": "./src/util.ts",
      "default": "./src/util.ts"
    }
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}

