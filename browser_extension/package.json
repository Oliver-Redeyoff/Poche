{
  "name": "poche-browser-extension",
  "version": "1.0.0",
  "description": "Poche - Read it later browser extension",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "build:chrome": "npm run build && cp -r dist chrome",
    "build:firefox": "npm run build && cp -r dist firefox",
    "build:safari": "npm run build && cp -r dist safari",
    "icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-bugfix-firefox-class-in-computed-class-key": "^7.28.5",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^9.1.3",
    "canvas": "^2.11.2",
    "copy-webpack-plugin": "^11.0.0",
    "typescript": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
